package pod.q1.node3.impl;

import java.sql.DriverManager;
import java.sql.SQLException;

import pod.q1.node3.JdbcConnect;

public class JdbcConnectImpl implements JdbcConnect {

	@Override
	public String query(String params) {
		//
		try {
			Class.forName(DriverManager.class.getName());
			DriverManager.getConnection("jdbc:postgresql://192.168.99.100:5432/dbq1", "postgres", "123456");
		} catch (SQLException e) {
			e.printStackTrace();
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
		//
		return null;
	}

}
